{
    "name": "t4web/admin",
    "description": "ZF2 Admin module",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "admin"
    ],
    "homepage": "https://github.com/t4web/Admin",
    "authors": [
        {
            "name": "Sebaks"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/t4web/base.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sebaks/crud.git"
        }
    ],

    "require": {
        "php": ">=5.5",
        "zendframework/zend-console": "~2.5.0",
        "zendframework/zend-eventmanager": "~2.5.0",
        "zendframework/zend-modulemanager": "~2.5.0",
        "zendframework/zend-mvc": "~2.5.0",
        "zendframework/zend-paginator": "~2.5.0",
        "zendframework/zend-view": "~2.5.0",
        "sebaks/crud": "dev-master"
    },
    "require-dev" : {
        "phpunit/phpunit": "4.5.*"
    },
    "autoload": {
        "psr-0": {
            "T4webAdmin": "src/",
            "T4webAdminTest": "tests/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "minimum-stability": "dev"
}
